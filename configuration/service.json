{
  "services": {
    "sample":{
      "type":"swarm",
      "image": "sample:latest",
      "containerPort": 8000,
      "hostPort": 3000,
      "imageUri": "docker.pkg.github.com/rahultripathidev/sample-docker-node/sample-docker-node:latest",
      "init": 1,
      "kongConfig": {
        "route": "/swarm",
        "upstream": false,
        "service": "auth-service",
        "service_path": "/"
      }
    },
    "sample2":{
      "type":"standalone",
      "image": "sample:latest",
      "containerPort": 8000,
      "hostportRange": [3444,3002,3005,3009],
      "imageUri": "docker.pkg.github.com/rahultripathidev/sample-docker-node/sample-docker-node:latest",
      "init": 1,
      "kongConfig": {
        "route": "/ws",
        "upstream": true,
        "upstreamService":"ws.v1.ewar",
        "service": "auth-service",
        "service_path": "/"
      }
    }
  }
}
